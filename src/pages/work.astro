---
import Layout from '../layouts/Layout.astro';

import { getCollection } from 'astro:content';
const projects = await getCollection('projects');
---

<Layout>
	<h1>My projects</h1>
    <ul>
    {projects.map(project => (
        <li><a href={`/projects/${project.id}`}>{project.data.title}</a></li>
    ))}
    </ul>
</Layout>
